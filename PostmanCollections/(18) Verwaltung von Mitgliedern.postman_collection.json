					"script": {
						"exec": [
							"pm.sendRequest(",
							"    {",
							"        url:'http://localhost:8080/session',",
							"        method:'POST',",
							"        body: {",
							"            mode: 'raw',",
							"            raw: JSON.stringify({",
							"                email: 'Markus.Mueller@gmail.com',",
							"                password:'2004'",
							"            })",
							"        },",
							"        header: {",
							"            'Content-Type': 'application/json'",
							"        }",
							"    }, (err, response) => {",
							"        if (err) {",
							"            console.error(err);",
							"            returm;",
							"        }",
							"        const token = response.headers.get('Authorization');",
							"        pm.request.headers.add({",
							"            key: 'Authorization',",
							"            value: token",
							"        });",
							"    }",
							");"
						],
